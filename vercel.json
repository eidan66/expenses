{
  "buildCommand": "yarn build",
  "installCommand": "yarn install",
  "outputDirectory": "dist/public",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "YARN_PRODUCTION": "false"
    }
  }
}

